{
  "version": 6,
  "include": [],
  "configurePresets": [
    {
      "name": "base",
      "hidden": true,
      "generator": "Ninja",
      "binaryDir": "build",
      "toolchainFile": "${sourceDir}/build_tools/cmake/toolchains/host-llvm.cmake",
      "cacheVariables": {
        "BUILD_SHARED_LIBS": "OFF",
        "CMAKE_BUILD_TYPE": "RelWithDebInfo",
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/install",
        "CMAKE_PROJECT_TOP_LEVEL_INCLUDES": "${sourceDir}/DependencyProvider.cmake",
        "CMAKE_PLATFORM_NO_VERSIONED_SONAME": "ON",
        "CPM_SOURCE_CACHE": "${sourceDir}/.cache.cpm",
        "CPM_USE_NAMED_CACHE_DIRECTORIES": "ON",
        "LLVM_BUILD_RUNTIME": "OFF",
        "LLVM_BUILD_TESTS": "OFF",
        "LLVM_BUILD_TOOLS": "ON",
        "LLVM_BUILD_UTILS": "ON",
        "LLVM_ENABLE_ASSERTIONS": "ON",
        "LLVM_ENABLE_BINDINGS": "OFF",
        "LLVM_ENABLE_DOXYGEN": "OFF",
        "LLVM_ENABLE_OCAMLDOC": "OFF",
        "LLVM_ENABLE_PROJECTS": "mlir",
        "LLVM_ENABLE_SPHINX": "OFF",
        "LLVM_ENABLE_TELEMETRY": "OFF",
        "LLVM_INCLUDE_BENCHMARKS": "OFF",
        "LLVM_INCLUDE_DOCS": "OFF",
        "LLVM_INCLUDE_EXAMPLES": "OFF",
        "LLVM_INCLUDE_RUNTIMES": "OFF",
        "LLVM_INCLUDE_TESTS": "ON",
        "LLVM_INCLUDE_TOOLS": "ON",
        "LLVM_INSTALL_TOOLCHAIN_ONLY": "ON",
        "LLVM_INSTALL_UTILS": "ON",
        "LLVM_APPEND_VC_REV": "OFF",
        "LLVM_LIT_ARGS": "-sv --time-tests",
        "LLVM_TARGETS_TO_BUILD": "NVPTX;Native",
        "MLIR_ENABLE_BINDINGS_PYTHON": "ON",
        "MLIR_LINK_MLIR_DYLIB": "OFF",
        "MLIR_TRT_ENABLE_NCCL": "OFF"
      }
    },
    {
      "name": "default",
      "inherits": "base"
    },
    {
      "name": "release",
      "inherits": "base",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "LLVM_ENABLE_ASSERTIONS": "OFF"
      }
    },
    {
      "name": "debug",
      "inherits": "base",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "minimal",
      "inherits": "base",
      "cacheVariables": {
        "MLIR_TRT_ENABLE_CUBLAS": "OFF",
        "MLIR_TRT_ENABLE_CUDA": "OFF",
        "MLIR_TRT_ENABLE_HLO": "ON",
        "MLIR_TRT_ENABLE_MPI": "OFF",
        "MLIR_TRT_ENABLE_NCCL": "OFF",
        "MLIR_TRT_ENABLE_TORCH": "OFF",
        "MLIR_TRT_TARGET_TENSORRT": "OFF"
      }
    },
    {
      "name": "github-cicd",
      "inherits": "base",
      "cacheVariables": {
        "ENABLE_ASAN": "$env{ENABLE_ASAN}",
        "LLVM_LIT_ARGS": "$env{LLVM_LIT_ARGS}",
        "MLIR_TRT_DOWNLOAD_TENSORRT_VERSION": "$env{MLIR_TRT_DOWNLOAD_TENSORRT_VERSION}"
      }
    },
    {
      "name": "distribution-wheels",
      "inherits": "release",
      "cacheVariables": {
        "MLIR_TRT_DOWNLOAD_TENSORRT_VERSION": "$env{MLIR_TRT_DOWNLOAD_TENSORRT_VERSION}",
        "MLIR_TRT_ENABLE_MPI": "OFF",
        "MLIR_TRT_ENABLE_NCCL": "OFF",
        "MLIR_TRT_ENABLE_TORCH": "ON"
      }
    }
  ]
}
